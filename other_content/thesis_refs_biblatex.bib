	urldate = {2020-09-30},
	date = {2007},
	file = {PDF:C\:\\Users\\treimer\\Zotero\\storage\\DSFQ5VQN\\Gaylord et al. - 2007 - Spatial patterns of flow and their modification within and around a giant kelp forest.pdf:application/pdf;PDF:C\:\\Users\\treimer\\Zotero\\storage\\75LWNRKY\\Gaylord et al. - 2007 - Spatial patterns of flow and their modification within and around a giant kelp forest.pdf:application/pdf},
