	file = {ScienceDirect Full Text PDF:C\:\\Users\\treimer\\Zotero\\storage\\CNA9B5NW\\Gjertsen et al. - 2020 - Access to areas for algae cultivation in Norway.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\treimer\\Zotero\\storage\\C3PG6PXM\\S0308597X19306050.html:text/html},
