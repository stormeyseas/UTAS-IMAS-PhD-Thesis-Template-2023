}

@article{groscolas_long-term_2001,
	title = {Long-term fasting and re-feeding in penguins},
	volume = {128},
	issn = {1095-6433},
	url = {https://doi.org/10.1016/S1095-6433(00)00341-X},
	series = {The Physiological Consequences of Feeding in Animals},
	abstract = {Spontaneous fasting during reproduction (sometimes with a full stomach) and moult is a major characteristic of the annual cycle of penguins. Long-term fasting (up to four months in male emperor penguins) is anticipated by the accumulation of fat (incubation fast) and of fat and protein (moult fast). During most of the incubation fast, birds rely almost entirely on lipids as an energy source, body proteins being spared. However, below a critical (but non-total) fat store depletion, marked behavioural, metabolic, and endocrine changes occur. Spontaneous locomotor activity increases and the egg is transitorily left unincubated for increasingly long periods, until its definitive abandon and the bird departs to re-feed at sea. These changes are thought to be activated by an endogenous re-feeding signal triggered before lethal energy depletion. An increase in body protein catabolism in the face of a reduction in lipid availability and utilisation, and an increase in circulating corticosterone vs. a decrease in plasma prolactin, are likely to be major metabolic and hormonal components of this signal. The survival and rapid restoration of energy stores in birds having departed to re-feed at a stage of near total lipid depletion demonstrates the effectiveness of the re-feeding signal. Penguins, and possibly other seabirds, are therefore appropriate animal models for understanding the long-term interactions between body energy reserves and fasting, breeding and feeding physiology and behaviour.},
	pages = {643--653},
	number = {3},
	journaltitle = {Comparative Biochemistry and Physiology Part A: Molecular \& Integrative Physiology},
	author = {Groscolas, René and Robin, Jean-Patrice},
	urldate = {2023-01-04},
	date = {2001},
	langid = {english},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\treimer\\Zotero\\storage\\WQUKG89F\\Groscolas and Robin - 2001 - Long-term fasting and re-feeding in penguins.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\treimer\\Zotero\\storage\\KK2QRPFE\\S109564330000341X.html:text/html},
}

@article{harter_substitution_2011,
