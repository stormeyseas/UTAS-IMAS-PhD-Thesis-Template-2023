}

@article{bernard_validation_2012,
	title = {Validation of a simple model accounting for light and temperature effect on microalgal growth},
	volume = {123},
	issn = {0960-8524},
	url = {https://doi.org/10.1016/j.biortech.2012.07.022},
	abstract = {A new model is set up to represent the effect of temperature and light on microalgae growth, and predict the productivity of outdoor microalgae based processes. This model includes three cardinal temperatures (Tmin, Topt, and Tmax) and three parameters associated to the light response (μmax, Iopt and α). These six parameters have a clear biological meaning which makes model calibration straightforward. An algorithm to estimate both the model parameters and their associated confidence regions is developed. Results show that this model can successfully represent experimental data sets from 15 species cultivated in different experimental conditions. The model predictions, when compared to realistic temperatures recorded in outdoor photobioreactors or raceways, point out a strong decrease of productivity due to over-warming for most commercial species.},
